							 mat[0][2] = mat[0][2] - mat[1][2];
							 mat[0][3] = mat[0][3] - mat[1][3];
							 mat[0][4] = mat[0][4] - mat[1][4];
							 mat[0][5] = mat[0][5] - mat[1][5];
							 mat[0][6] = mat[0][6] - mat[1][6];
							 mat[0][7] = mat[0][7] - mat[1][7];
							 mat[0][8] = mat[0][8] - mat[1][8];
							 mat[0][9] = mat[0][9] - mat[1][9];
						 end
					end
				end
endmodule		
